generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
}

model badges {
  id_badge     String         @id 
  slug         String
  name         String
  image        String
  user user[]
}

model user {
  id_user      String         @id @default(uuid()) 
  email        String
  password     String
  badges badges[]
}